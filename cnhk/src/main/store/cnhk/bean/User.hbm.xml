<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="store.cnhk.bean.User" table="user" schema="cnhk">
        <id name="userId" column="userid"/>
        <property name="userName" column="username"/>
        <property name="userPassward" column="userpassward"/>
        <property name="wechatid" column="wechatid"/>
        <property name="wechatName" column="wecharname"/>
    </class>
</hibernate-mapping>